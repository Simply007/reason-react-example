{
  "name": "reason-react-example",
  "bsc-flags": [
    "-bs-no-version-header",
    "-bs-super-errors"
  ],
  "reason": {
    "react-jsx": 2
  },
  "namespace": true,
  "bs-dependencies": [
    "reason-react",
    "reason-apollo",
    "bs-fetch",
    "@glennsl/bs-json"
  ],
  "ppx-flags": [
    "graphql_ppx/ppx"
  ],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "refmt": 3
}